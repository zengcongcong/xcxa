/**index.wxss**/
@import "/common/style/stylesheet.wxss";

 .container {
  width: 100%;
  min-height: 100%;
  display: flex;
  overflow-x: hidden;
  flex-direction: column;
  box-sizing: border-box;
  background-color: #f7f7f7; 
} 
.titless{
  font-size: 28rpx;
}
.idonss{
  z-index: 99999;
}
.search-box{
  width: 100%;
  margin-top: 10rpx;
}
.search-txt{
  width: 90%;
  height: 68rpx;
  margin: 0 auto;
  position: relative; 
  border:solid 1px #ddd;  
  border-radius: 45px; 
  background-color: #fff; 
}
.btn-search{
  position: absolute;  
  right:24rpx;
  top:22rpx;
  color: rgb(25, 137, 209);
}
#search{
  padding-left: 28rpx;
  font-size: 28rpx;
  height: 68rpx;
  line-height: 68rpx;
}

 swiper{
  width: 100%;
  margin-top: 10rpx;
   height: 300rpx; 
} 
.slider-item{
    font-size: 28rpx;
    position: relative;
    width: 100%;
    height: 100%;
    vertical-align: top;
    white-space: normal;
}
.slider-item image{
  width: 100%;
  min-width: 830rpx;
  height: 100%;
}
.slider-title{
  line-height: 30px;  
  position: absolute;
  bottom: 0;
  left: 0;
  font-size: 24rpx;
  color:#8f8f94;
  width: 100%;
  height: 30px;
  margin: 0;
  text-align: left;
  text-indent: 12px;
  opacity: .8;
  background-color: #000; 
}

.center-ss{
  width: 100%;
  margin-top: 10rpx;
}
.List{
    padding: 5px 0;
    width: 100%;
    overflow: hidden;
    background-color: #fff;
    display: -webkit-flex;

}
.imagess{
    flex: 1;
    margin: 5px;
    text-align: center;
    text-transform: uppercase;
}
.list-box {
    width: 90rpx;
    height:  90rpx;
    text-align: center;
    line-height:  90rpx;
    border-radius: 50%;
    margin: 0 auto;
    background-color: lightgray;
    justify-content: center;
}

.list-box>image{
    width: 90rpx;
    height:  90rpx;
    border-radius: 50%;
    border: 0;
}

.imagess>text{
    color: #121212;
    margin: 8px 0 0 0;
    line-height: 1;
    font-size: 24rpx;
}
.rank-shop-wrap {
    margin-top: 12rpx;
}
.rank-shop-inner {
    background-color: #fff;
    padding-bottom: 20rpx;
}
#shop-list{
  width: 100%;
}
.titles{
    padding: 10px;
    border-bottom: 1px solid rgb(201, 201, 201);
}
.titles>.text{
    border-left: 3px solid cornflowerblue;
    padding-left: 3px;
    font-size: 28rpx;
    line-height: 1;
}
.shop-content{
    width: 100%;
    overflow: hidden;
    margin-top: -1px;
}
.content-box {
    width: auto;
    margin-right: -10rpx;
    overflow: hidden;
    font-size: 0;
}
.table-edl {
    position: relative;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    background-color: #fff;
}
.shop-content{
    width: 100%;
    overflow: hidden;
    margin-top: -1px;
}
.content-text{
    display: inline-block;
    width: 42%;
    /* float: left; */
    font-size: 14px;
    padding: 10px 12px;
    border-top: 1px solid #EDEDED;
    border-right: 1px solid #EDEDED;
}
.flex-box-align-center{
 display: -webkit-flex;
}
.mui-media-body{
    width: 30%;
    flex: 1; 
    padding-right: 6px;
}
 .mall-img-object {
    max-width: 56px;
    height: 56px;
    line-height: 56px;
}
.mui-ellipsis{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.mui-media-body>.up{
    font-size: 24rpx;
    font-weight: normal;
    color: #000;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
}
.mui-media-body>.downs{
    font-size: 24rpx;
    margin-top: 0;
    margin-bottom: 10px;
    color: #8f8f94;
}
.hot-shops {
    width: 100%;
    background-color: #fff;
    margin-top: 10rpx;
}
.hot-title {
    color: #de2727;
    font-size: 28rpx;
    position: relative;
    text-align: center;
    padding: 20rpx 0;
}
.shu-l,.shu-r{
    width: 38rpx;
    height: 1px;
    background-color: #de2727; 
    top: 50%;
    position: absolute;
}
 .shu-l{
       left: 35%;
          
}
 .shu-r{
   right: 35%;
}  
.list-item{
  width:100%;
  overflow:hidden;
  box-sizing:border-box;
  padding:0 20rpx;
}
.movie-item {
width:48%;
margin:1%;
float:left;
}
.movie-item .poster {
width:100%;
}
.movie-item image {
width: 100%;
height: 340rpx;
min-height: 340rpx;
max-width: 100%;
text-align: center;
}
.movie-item .title{
font-size:28rpx;
text-align:center;
color:#333;
margin:5rpx 0;
}
.title{
    padding: 8rpx 0;
    width: 100%;
    overflow: hidden;
    display: -webkit-flex;
}
.title>.shop-type1{
    max-width: 140rpx;
    color: #fff;
    font-size: 20rpx;
    padding: 0 12rpx;
    border-radius: 16rpx;
    display: inline-block;
    background-color: rgb(235, 39, 13);
    overflow: hidden;
    white-space: nowrap;
    text-align: center;
    height: 44rpx;
    line-height: 44rpx;

}
.shop-type2 {
    color: rgb(38, 70, 212);
    font-size: 22rpx;
    text-align: center;
    padding: 0 6rpx;
    line-height: 40rpx;
}
.shop-title{
  font-size: 24rpx;
}
.shop-price{
  font-size: 24rpx;
  color: #de2727;
}